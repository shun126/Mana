void TestStatementFor()
{
    int j;
    for(j = 0; j < 10; ++j)
    {
        print("%d\n", j);
    }
    for(int i = 0; i < 10; ++i)
    {
        print("%d\n", i);
    }
    for(int k = 0; k < 10; ++k)
    {
        print("%d\n", k);
    }
}

