// global initializers
int constValue = 3 + 4;
float constFloat = 1.5;
int orderedA = 1;
int orderedB = orderedA + 1;

int RuntimeValue()
{
	return 563;
}

int runtimeValue = RuntimeValue();

actor Test
{
	action init
	{
		print("constValue: %d\n", constValue);
		print("constFloat: %f\n", constFloat);
		print("orderedB: %d\n", orderedB);
		print("runtimeValue: %d\n", runtimeValue);

		// touch globals so code generation exercises references
		constValue = constValue + orderedB;
		constFloat = constFloat + 0.5;
		orderedB = orderedA + 2;
		runtimeValue = RuntimeValue();
	}
}
