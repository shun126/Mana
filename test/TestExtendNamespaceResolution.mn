/*
mana (script)
*/

// global module
module BaseModule
{
    action init
    {
    }
}

// module in another namespace, used via `using`
namespace B
{
    module ExtraModule
    {
        action init
        {
        }
    }
}

using B;

namespace A
{
    actor FromGlobal
    {
        extend BaseModule;
    }

    actor FromUsing
    {
        extend ExtraModule;
    }
}
