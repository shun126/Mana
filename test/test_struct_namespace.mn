////////////////////////////////////////////////////////////////////////////////
// global struct
struct GlobalStruct
{
	int g;
}

////////////////////////////////////////////////////////////////////////////////
// namespaced struct
namespace A
{
	struct NsStruct
	{
		int n;
	}
}

using A;

////////////////////////////////////////////////////////////////////////////////
// global variables
GlobalStruct globalValue;
A::NsStruct namespacedValue;

////////////////////////////////////////////////////////////////////////////////
// local variables (using namespace)
actor Root
{
	action main
	{
		GlobalStruct localGlobal;
		NsStruct localNamespaced;
		A::NsStruct localQualified;
	}
}
