void test_statement_switch()
{
    short hoge = 0;
    switch(hoge)
    {
    case 0:
        print("0\n");
        break;
    case 1:
        print("1\n");
        break;
    case 2:
        print("2\n");
        break;
    default:
        print("%d\n", hoge);
        break;
    }
}
